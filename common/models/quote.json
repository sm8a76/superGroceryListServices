{
  "name": "Quote",
  "plural": "quotes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "price": {
      "type": "number",
      "required": true,
      "default": "0"
    },
    "promo": {
      "type": "string"
    },
    "unit": {
      "type": "string"
    },
    "discountIfAny": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "grocery": {
      "type": "belongsTo",
      "model": "Grocery",
      "foreignKey": "groceryId"
    },
    "supermarket": {
      "type": "belongsTo",
      "model": "Supermarket",
      "foreignKey": "superId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
